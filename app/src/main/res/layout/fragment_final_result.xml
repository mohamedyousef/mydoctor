<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.FinalResultFragment">
    <android.support.design.widget.AppBarLayout android:id="@+id/tool10"
        android:layout_width="match_parent"
        android:layout_alignParentTop="true"
        android:layout_height="wrap_content" >

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </android.support.v7.widget.Toolbar>

    </android.support.design.widget.AppBarLayout>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="نتيجة التشخيص" android:id="@+id/final_result"
        android:layout_below="@+id/tool10"
        android:layout_marginTop="50dp"
        android:textSize="36sp"
        android:textAlignment="center"
        tools:ignore="NotSibling" />


</RelativeLayout>